{
  "framework": "vite",
  "installCommand": "pnpm --dir .. install --filter @home/shared --filter @home/web --frozen-lockfile",
  "buildCommand": "pnpm --dir .. --filter @home/web build",
  "outputDirectory": "dist",
  "routes": [
    { "handle": "filesystem" },
    { "src": "/.*", "dest": "/index.html" }
  ]
}
